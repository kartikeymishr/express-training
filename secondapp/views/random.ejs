<%- include('partials/head') %>
<body>
<%- include('partials/navbar') %>
<div class="container">
    <h1>Your random number is <%= randomNumber; %></h1>
    <% if (randomNumber % 2 === 0) { %>
        <h2>That is an even number</h2>
    <% } else { %>
        <h2>That is an odd number</h2>
    <% } %>
    <%= randomNumber % 2 === 0 ? 'EVEN' : 'ODD' %>
</div>

</body>
</html>